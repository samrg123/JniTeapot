{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "logcat",
	"patterns": [
		{
			"include": "#verbose"
		},
		{
			"include": "#info"
		},
		{
			"include": "#debug"
		},
		{
			"include": "#warning"
		},
		{
			"include": "#error"
		},
		{
			"include": "#assert"
		}
	],
	"repository": {
		"verbose": {
			"patterns": [{
				"name": "markup.other.logcat.verbose",
				"begin": "^(([\\d-]+?)\\s+)?(([\\d:\\.]+?)\\s+)?((\\d+)(\\s+|-)(\\d+)(\/)?)?(([\\w\\.?]+?)?\\s+)?(V)((\\s+|\/)(.+?))?:\\s+(.*)$",
				"beginCaptures": {
					"2": {"name": "entity.date.logcat"},
					"4": {"name": "entity.time.logcat"},
					"6": {"name": "entity.pid.logcat"},
					"8": {"name": "entity.tid.logcat"},
					"11": {"name": "entity.package.logcat"},
					"12": {"name": "entity.level.logcat"},
					"15": {"name": "entity.tag.logcat"},
					"16": {"name": "entity.message.logcat"}
				},
				"contentName": "logcat.message",
				"end": "(?=^(([\\d-]+?)\\s+)?(([\\d:\\.]+?)\\s+)?((\\d+)(\\s+|-)(\\d+)(\/)?)?(([\\w\\.?]+?)?\\s+)?([VIEWAD])((\\s+|\/)(.+?))?:\\s+(.*)$)"
			}]
		},
		"info": {
			"patterns": [{
				"name": "markup.quote.logcat.info",
				"begin": "^(([\\d-]+?)\\s+)?(([\\d:\\.]+?)\\s+)?((\\d+)(\\s+|-)(\\d+)(\/)?)?(([\\w\\.?]+?)?\\s+)?(I)((\\s+|\/)(.+?))?:\\s+(.*)$",
				"beginCaptures": {
					"2": {"name": "entity.date.logcat"},
