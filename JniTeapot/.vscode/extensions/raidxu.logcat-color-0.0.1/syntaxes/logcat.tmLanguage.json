				"begin": "^(([\\d-]+?)\\s+)?(([\\d:\\.]+?)\\s+)?((\\d+)(\\s+|-)(\\d+)(\/)?)?(([\\w\\.?]+?)?\\s+)?(W)((\\s+|\/)(.+?))?:\\s+(.*)$",
				"beginCaptures": {
					"2": {"name": "entity.date.logcat"},
					"4": {"name": "entity.time.logcat"},
					"6": {"name": "entity.pid.logcat"},
					"8": {"name": "entity.tid.logcat"},
					"11": {"name": "entity.package.logcat"},
					"12": {"name": "entity.level.logcat"},
					"15": {"name": "entity.tag.logcat"},
					"16": {"name": "entity.message.logcat"}
				},
				"contentName": "logcat.message",
				"end": "(?=^(([\\d-]+?)\\s+)?(([\\d:\\.]+?)\\s+)?((\\d+)(\\s+|-)(\\d+)(\/)?)?(([\\w\\.?]+?)?\\s+)?([VIEWAD])((\\s+|\/)(.+?))?:\\s+(.*)$)"
			}]
		},
		"error": {
